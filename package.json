{
  "name": "spl-language-support",
  "displayName": "spl-language-support",
  "description": "language support for SPL",
  "version": "1.1.1",
  "engines": {
    "vscode": "^1.0.0"
  },
  "author": {
      "name": "movieminer"
  },
  "publisher": "movieminer",
  "repository": {
      "url": "https://github.com/movieminer/spl-language-support",
      "type": "git"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "spl",
        "aliases": [
          "SPL",
          "spl"
        ],
        "extensions": [
          ".spl"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "spl",
        "scopeName": "source.spl",
        "path": "./syntaxes/spl.tmLanguage.json"
      }
    ]
  }
}
